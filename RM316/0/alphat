FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    outlet {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    top {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    bottom {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    left {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    right {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    volume
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
}


