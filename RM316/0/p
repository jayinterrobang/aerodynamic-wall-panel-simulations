FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    walls 
    {
        type            zeroGradient;
    }


    inlet 
    {
        type            zeroGradient;
    }

    outlet 
    {
        type            zeroGradient;
    }

    volume 
    {
        type            zeroGradient;
    }
}

