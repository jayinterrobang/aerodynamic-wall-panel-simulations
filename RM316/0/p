FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 0;
    }

    bottom 
    {
        type            zeroGradient;
    }

    inlet 
    {
        type            zeroGradient;
    }

    outlet 
    {
        type            zeroGradient;
    }

    left 
    {
        type            zeroGradient;
    }

    right 
    {
        type            zeroGradient;
    }

    volume 
    {
        type            zeroGradient;
    }
}

