FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000765;

boundaryField
{
    inlet 
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    volume 
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    top 
    {
        type            zeroGradient;
    }
    bottom 
    {
        type            zeroGradient;
    }
    left 
    {
        type            zeroGradient;
    }
    right 
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
